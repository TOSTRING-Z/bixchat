        {
          "version": "Pro/Qwen/Qwen2-VL-7B-Instruct",
          "vision": [
            "image"
          ]
        }
      ]
    }
  },
  "plugins": {
    "python_execute": {
      "params": {
        "python_bin": "python",
        "delay_time": 10,
        "threshold": 10000
      },
      "enabled": false
    },
    "file_load": {
      "extra": [
        {
          "type": "file-reader"
        }
      ],
      "show": false,
      "params": {
        "threshold": 10000
      },
      "enabled": false
    },
    "search_files": {
      "enabled": false
    },
    "list_files": {
      "params": {
        "threshold": 50
      },
      "enabled": false
    },
    "write_to_file": {
      "enabled": false
    },
    "replace_in_file": {
      "enabled": false
    }
  },
  "mcp_server": {
    "biotools": {
      "url": "http://172.27.0.3:3001/sse",
      "enabled": true
    }
  },
  "chain_call": [
    {
      "model": "plugins",
      "version": "python_execute",
      "input_data": {
        "code": "{query}"
      },
      "end": true
    }
  ],
  "tool_call": {
    "memory_length": 20,
    "mcp_timeout": 600,
    "extra_prompt": "biotools/mcp_server/prompt.md",
    "llm_parmas": {
      "max_tokens": 8000,
      "temperature": 0.5,
      "stream": true,
      "response_format": {
        "type": "json_object"
      }
    }
  }
}